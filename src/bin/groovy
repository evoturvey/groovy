#!/bin/sh

##############################################################################
##                                                                          ##
##  Groovy JVM Bootstrap for UN*X                                           ##
##                                                                          ##
##  use -cp or -classpath just as in java to use a custom classpath         ##
##                                                                          ##
##############################################################################

##
## $Revision$
## $Date$
##

DIRNAME=`dirname "$0"`
export JAVA_OPTS="$JAVA_OPTS -Dscript.name=$0"
. "$DIRNAME/startGroovy"

startGroovy groovy.ui.GroovyMain "$@"
